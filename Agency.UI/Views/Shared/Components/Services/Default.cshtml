
@model List<Services>

    <div class="row">
        @foreach (var item in Model)
        {


        <div class="col-md-3 col-sm-3 col-xs-12 text-xs-center wow fadeInDown" data-wow-delay=".2s" >
            <div class="service-box">
                <div class="icon">
                    <i class="fa fa-support"></i> <h3>@item.Name</h3>
                </div>
                <p>@item.Description</p>
            </div>
        </div>
        }

    </div>